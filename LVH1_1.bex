		            Sleep 1,TVBLANK
		        loop
    		    PosRocx=PosJoueurX(PosJ)
    		    PosRocy=PosJoueurY(PosJ)
		    end if
		    
		    if CHJ=TirJ then
		        if PosJ>1 then
		            LoadTiles DeMonstre_1,16,AdresseDe
    				LoadTiles DeMonstre_1,16,AdresseDe-16
     		        PropSprite SpriteDe,AdresseDe+Priority(1),0
    	            PropSprite SpriteDe1,AdresseDe+Priority(1)-16,0
    	        else if PosJ=1 then
		            LoadTiles  TileDe1,16,AdresseDe
    				LoadTiles  TileDe1,16,AdresseDe-16
     		        PropSprite SpriteDe,AdresseDe+Priority(1),2
    	            PropSprite SpriteDe1,AdresseDe+Priority(1)-16,2
		        end if
		        TempoPosJ=0
		        DeltaTempoJ=0
		        PhaseJeuM=PhaseJeu
		        PhaseJeu=99
		        gosub GongSound
		        PrinttextCombat 4,24+decalY,"DEBUT PARTIE              "
		        PrintText16 19,11,str$(NombreTour),1
		    end if 
		end if
		
		'Debut Partie - Lancer de dés
		if PhaseJeu=5 then
		    'Cas de l'IA
		    if PosJ>0 then
		        'Affichage et Init
		        if aff=0 then 
		            if PosJ!=1 then 
		                PrinttextCombat 4,24+decalY,"TOUR DE JOUEUR "+str$(PosJ)+"             "
		            else
		                PrinttextCombat 4,24+decalY,"VOTRE TOUR                   "
		            end if
		            PrintText16 19,11,str$(NombreTour),1
		            aff=1
    		        if PosJ>1 then
    		            LoadTiles DeMonstre_1,16,AdresseDe
        				LoadTiles DeMonstre_1,16,AdresseDe-16
         		        PropSprite SpriteDe,AdresseDe+Priority(1),0
        	            PropSprite SpriteDe1,AdresseDe+Priority(1)-16,0
        	        else if PosJ=1 then
    		            LoadTiles  TileDe1,16,AdresseDe
        				LoadTiles  TileDe1,16,AdresseDe-16
         		        PropSprite SpriteDe,AdresseDe+Priority(1),2
        	            PropSprite SpriteDe1,AdresseDe+Priority(1)-16,2
    		        end if
		        end if
		        
    			TempoVitesse++
    			if Velocite=0 and TempoVitesse=1 then Gosub DeSound
    			RandomSeed++
    			if joy then randomize Randomseed
    			Tirage=Rnd(5)
    			Ti1=Rnd(10)
    			If Ti1>5 And Tirage=4 Then Tirage=5
