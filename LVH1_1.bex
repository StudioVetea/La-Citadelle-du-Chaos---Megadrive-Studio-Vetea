'//////////////////////////////////////////////
Declare Sub PrintTextCombat(x As Integer, y As Integer, tex$ As String,P As Integer)
	x++
	y++
	l = Len(tex$)
	Dim Tex1$ As String
	Tex1$=tex$
	DataPtr& = VarPtr&(tex$)
	SetGfxPlane Scroll_A
	For i=1 To l
		Read t
		if t>=61 then DA=55 else DA=49
		if t!=32 then 
		    DrawTile AdresseFontCombat+t-DA+Palette(0)+Priority(0),x,y
		else
		    DrawTile 0+t+Palette(0)+Priority(0),x,y
		end if
		x++
	Next
End Sub

