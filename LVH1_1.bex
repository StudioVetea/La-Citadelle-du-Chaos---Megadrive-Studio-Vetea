WriteBook:
	gosub CreatePointeur
	AdresseFontCombat=1300
	loadtiles FontPage,10,AdresseFontCombat
	Palettes PaletteBase,0,0,16
     'Font Page
	'locate 0,36:print Page
	if Page>0 then printtextPage 36,0,str$(Page),1
	'Locate 0,32: print LinkPage(Page)
	'if Page>10 then printtextPage 20,0,str$(LinkPage(Page)),1
	'if Page>11 then printtextPage 1,0,str$(LinkPage(Page-1)),1

	i=0
	Tamp$=""
	j=0
	a=0
	TemG=0
	ExitPage=0
	if decalversion=0 then DecalY=1 else DecalY=1
	
	'0 : Droite, 1 : Gauche
	'D?lement rapide.
	if (TexteSpeed=1 or ReadPage(Page)=1) and Lo>0 then 
		loadtiles PointeurGantD,16,AdressePointeur
		while (j<Lo)
			Tamp$=mid$(Mess$(j),0,35)
			printtext TexteX,TexteY+DecalY+j,Tamp$,1
			a=0
			Tamp$=""
			j++
		wend
	end if
	
	Tempo=0
	Cur=0
	TemS=0
	'Boucle principale
	do 
		joy=joypad(0)
		
		'Changement Font
		if Joy.5 and FontChange=0 then gosub ChangeFont:FontChange=1
		if Joy=0 then FontChange=0
			
        Gosub GestionSonore

		if (texteSpeed=0 and j<Lo) and Lo>0 then
			tempo++
			if i>35 then
				Tamp$=""
				printtext TexteX+34,TexteY+DecalY+j,"   ",1
				i=0
				j++  
			end if
			
			'On fait une avance rapide sur les Espaces Textes.
			if mid$(Mess$(j),i+1,1)=" " then Speed=10 else speed=50
            
			'Avance Rapide
			if tempo>2 and (joy.7 or joy.6 or joy.4) then Speed=10 

			if tempo>Speed then
				Tamp$=Tamp$+mid$(Mess$(j),i,1)
				if cur=0 then printtext TexteX+1+i,TexteY+DecalY+j,"[",1 else printtext TexteX+1+i,TexteY+DecalY+j,"]",1
				cur++
				if cur>1 then cur=0
				printtext TexteX,TexteY+DecalY+j,Tamp$,1
				i++
				Tempo=0
			end if
		end if
		
		'Fin du Texte ?? Sprite Page Suivante
		if TexteSpeed=1 or j>=Lo or Lo=0 then
			'En mode Aventure, on peut acc?r ?a feuille d'aventure
			if PartieRapide=0 and joy.7 and ChancePage=0 and tems>10 and Page>=11 and Lo>0 and FontChange=0 then gosub GestionFeuille
			if QuestionPage=0 then 
				movesprite PointeurS,128+320-32-4,128+212-GS
				if Page>0 then movesprite PointeurS1,128+4,128+212-GS
				
				if joy.3 then 
					if CombatFeuille(Page)!=1 and ChancePage=0 then Page++:ExitPage=3:Gosub PageSound
					exit do
				end if
				
				if joy.2 and Page>0 then 
					ReadPage(Page)=1
					if CombatFeuille(Page)=1 then CombatFeuille(Page)=0
					Page--
					Gosub PageSound
					ExitPage=1
					exit do
				end if
				
				if TemS<10 then loadtiles PointeurGantD,16,AdressePointeur else loadtiles PointeurGantD1,16,AdressePointeur
				if TemS<10 and Page>0  then loadtiles PointeurGantG,16,AdressePointeur-16 else loadtiles PointeurGantG1,16,AdressePointeur-16
				TemS++
				if TemS>20 then TemS=0
			else
				exit do
			end if
		end if
		if (TexteSpeed=1 or j>=Lo or Lo=0) then sleep 1,TVBlank
	loop
	freeallsprites
	

	'Liaison Page & Pages Lues
	ReadPage(Page-1)=1

	'Combat ?
	if CombatFeuille(Page)=1 and ExitPage!=1 then gosub ModuleCombat
	
	Gosub LiaisonPage 
	return
	
	
	
