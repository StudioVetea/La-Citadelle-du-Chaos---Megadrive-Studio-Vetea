	'Sprites
	SpriteDe=AddSprite(4,4)
	SpriteDe1=AddSprite(4,4)
	PointeurS=addsprite(4,4)
	SpriteRoc=addsprite(4,4)
	Ad=0
	for i=1 to 6
	    PosyD(i)=184+DecalVersion
	    PosXD(i)=25
	    TempoDague(i)=0
	    FrameID(i)=0
	    S_DagueD(i)=addsprite(4,4)
	    loadtiles DagueSprite1,16,AdresseDague+(ad<<4)
	    propsprite S_DagueD(i),AdresseDague+priority(1)+(ad<<4),3
	    Ad++
	next
	PropSprite SpriteDe,AdresseDe+Priority(1),2
	propsprite PointeurS,AdressePointeurCombat+priority(1),3
	PropSprite SpriteDe,AdresseDe+Priority(1),2
	PropSprite SpriteDe1,AdresseDe+Priority(1)-16,0
	
	'Init variables
    Ad=0
    TirDague=0
    PhaseJeuD=0
    IDP=0
    GoAnimation=0
    for i=1 to 6
        XD(i)=200+(i<<4)+Ad
        Ad=Ad+8
    next
    Ad=0
	CYDe=300+DecalVersion
	CXDe=1 'Max=184
	TempoVitesse=0
	Velocite=0
	Vitesse=19
	aff=0
	Ready=0
	GoAction=0
	Tempo=0
	DY=0
	DX=12
	Pause=0
	PhaseJeuD=0
	WinGame=0
	SituationSound=2
	stopplay 0
	
	'Affichage Icone
	DRAWTILESINC2 AdresseTileCombat+Palette(3)+priority(0),19+8,20+decalY,4,4
	PrinttextCombat 6,24+decalY,"COMMENCEZ LA PARTIE       "
    
    'Boucle principale
    Do
	    gosub gestionsonore
		joy=joypad(0)
		if joy then randomize Randomseed
        Randomseed++
        

		if Ready!=0 then Joy=0
		if joy then randomize Randomseed
		if joy=0 then tempo++
		if tempo>5 then Tempo=5

        if GoAction=0 then
    		'Pointeur
    		if Ready=0 then 
    		    pointeurGantS 3+(DX<<1),22+DecalY-(DY<<2)
    		end if
    		
    		'Gauche
    		if joy.2 and tempo>2 and PhaseJeuD=0 then
    			Tempo=0
    			InfoTexte=0
    			DX=12
    			loadtiles EpeeCombat1S,16,AdresseTileCombat
    			loadtiles FuiteCombatS,16,AdresseTileFuite
    			if PhaseJeu=0 then 	PrinttextCombat 6,24+decalY,"COMMENCEZ LA PARTIE       "
    		end if
    		
    		if (joy.6 or joy.5 or joy.4) and tempo>2 and PhaseJeuD=0 and DX=12 then 
    		    PhaseJeuD=99
    		    gosub GongSound
    		    Tempo=0
    		    Goaction=1
    		    movesprite PointeurS,1,1
    		end if
    		
    		'Push Pointeur
    		if (joy.6 or joy.5 or joy.4) then loadtiles PointeurGant1,16,AdressePointeurCombat else loadtiles PointeurGant,16,AdressePointeurCombat 
    		'Validation pointeur
    		if (joy.6 or joy.5 or joy.4) and tempo>2 and valid=0 then valid=1:tempo=0:Gosub ValidationSound
		end if
        
        'Gestion Dé
		if PhaseJeuD=3 then
	        'Affichage et Init
	        if aff=0 then 
	            if IDP= then 
	                if IdJoueur=1 then PrinttextCombat 6,24+decalY,"TOUR DE HANZO           "
	                if IdJoueur=2 then PrinttextCombat 6,24+decalY,"TOUR DE DILZUG          "
	                if IdJoueur=3 then PrinttextCombat 6,24+decalY,"TOUR DE MERLAG          "
	                if IdJoueur=4 then PrinttextCombat 6,24+decalY,"TOUR DE KRAYG           "
	            else
	                PrinttextCombat 6,24+decalY,"VOTRE TOUR               "
	            end if
	            'PrintText16 19,11,str$(NombreTour),1
	            aff=1
		        if IDP=1 then
		            LoadTiles DeMonstre_1,16,AdresseDe
    				LoadTiles DeMonstre_1,16,AdresseDe-16
     		        PropSprite SpriteDe,AdresseDe+Priority(1),0
    	            PropSprite SpriteDe1,AdresseDe+Priority(1)-16,0
    	        else if IDP=0 then
		            LoadTiles  TileDe1,16,AdresseDe
    				LoadTiles  TileDe1,16,AdresseDe-16
     		        PropSprite SpriteDe,AdresseDe+Priority(1),2
    	            PropSprite SpriteDe1,AdresseDe+Priority(1)-16,2
		        end if
	        end if
	        
			TempoVitesse++
			if Velocite=0 and TempoVitesse=1 then Gosub DeSound
			RandomSeed++
			if joy then randomize Randomseed
			Tirage=Rnd(5)
			Ti1=Rnd(10)
			If Ti1>5 And Tirage=4 Then Tirage=5
			RandomSeed++
			Tir=(Tirage)+1
			if TempoVitesse>25 then 
				Velocite++
				TempoVitesse=0
			end if
		
			TempoVelocite++
			if TempoVelocite>2 then 
				if IDP=1 then
    				if Tirage=0 then LoadTiles DeMonstre_1,16,AdresseDe
    				if Tirage=1 then LoadTiles DeMonstre_2,16,AdresseDe
    				if Tirage=2 then LoadTiles DeMonstre_3,16,AdresseDe
    				if Tirage=3 then LoadTiles DeMonstre_4,16,AdresseDe
    				if Tirage=4 then LoadTiles DeMonstre_5,16,AdresseDe
    				if Tirage=5 then LoadTiles DeMonstre_6,16,AdresseDe
				else if IDP=0 then
    				if Tirage=0 then LoadTiles TileDe1,16,AdresseDe 
    				if Tirage=1 then LoadTiles TileDe2,16,AdresseDe
    				if Tirage=2 then LoadTiles TileDe3,16,AdresseDe
    				if Tirage=3 then LoadTiles TileDe4,16,AdresseDe
    				if Tirage=4 then LoadTiles TileDe5,16,AdresseDe
    				if Tirage=5 then LoadTiles TileDe6,16,AdresseDe
				end if
				Vitesse-=Velocite
				if vitesse>255 then vitesse=0
				CXDe+=Vitesse
				TempoVelocite=0
			end if
			
			'Fin du Tirage
			if CXDe>280 then
				CXDe=280
				if IDP=1 then
    				if Tirage=0 then LoadTiles DeMonstre_1,16,AdresseDe
    				if Tirage=1 then LoadTiles DeMonstre_2,16,AdresseDe
    				if Tirage=2 then LoadTiles DeMonstre_3,16,AdresseDe
    				if Tirage=3 then LoadTiles DeMonstre_4,16,AdresseDe
    				if Tirage=4 then LoadTiles DeMonstre_5,16,AdresseDe
    				if Tirage=5 then LoadTiles DeMonstre_6,16,AdresseDe
				else if IDP=0 then
    				if Tirage=0 then LoadTiles TileDe1,16,AdresseDe 
    				if Tirage=1 then LoadTiles TileDe2,16,AdresseDe
    				if Tirage=2 then LoadTiles TileDe3,16,AdresseDe
    				if Tirage=3 then LoadTiles TileDe4,16,AdresseDe
    				if Tirage=4 then LoadTiles TileDe5,16,AdresseDe
    				if Tirage=5 then LoadTiles TileDe6,16,AdresseDe
				end if
				Tir=(Tirage)+1
				PhaseJeuD=4
		        PrintText16 17,18,str$(Tir),1
		    end if
		end if
		
		'Gerbe de Sang : Mort !!
		if PhaseJeuD=50 then
            TempoDague(TirageDague)++
            if TempoDague(TirageDague)>7 then
                FrameID(TirageDague)++
                if FrameID(TirageDague)>9 then FrameID(TirageDague)=1:PhaseJeuD=5:GoAnimation=0:TempoDague(TirageDague)=0:FrameID(TirageDague)=1:Sleep 50
                if FrameID(TirageDague)=1 then loadtiles GerbeSang1,16,AdresseDague+((TirageDague-1)<<4)
                if FrameID(TirageDague)=2 then loadtiles GerbeSang2,16,AdresseDague+((TirageDague-1)<<4)
                if FrameID(TirageDague)=3 then loadtiles GerbeSang3,16,AdresseDague+((TirageDague-1)<<4)
                if FrameID(TirageDague)=4 then loadtiles GerbeSang4,16,AdresseDague+((TirageDague-1)<<4)
                if FrameID(TirageDague)=5 then loadtiles GerbeSang5,16,AdresseDague+((TirageDague-1)<<4)
                if FrameID(TirageDague)=6 then loadtiles GerbeSang6,16,AdresseDague+((TirageDague-1)<<4)
                if FrameID(TirageDague)=7 then loadtiles GerbeSang7,16,AdresseDague+((TirageDague-1)<<4)
                if FrameID(TirageDague)=8 then loadtiles GerbeSang8,16,AdresseDague+((TirageDague-1)<<4)
                if FrameID(TirageDague)=9 then loadtiles GerbeSang9,16,AdresseDague+((TirageDague-1)<<4)
                TempoDague(TirageDague)=0
            end if
		end if

        'Affichage Sprite
        Ad=0
        for id=1 to 6
            'DéBut Cycle
            if PhaseJeuD=99 then
                if PosXD(id)<=Xd(id) then PosXD(id)=PosXD(id)+4
                if PosXD(6)>=Xd(6) then PhaseJeuD=1:gosub SortsSound       
            end if
            'Tirage au sort de la dague
            if PhaseJeuD=1 then
                Tir=rnd(6)+1
                'sleep 50
                IDP++
                if IDP>1 then IDP=0
                if IDP=0 then CX=223 
                if IDP=1 then CX=336
                if Tir=0 then Tir=1
                if Tir>6 then Tir=6
                TirageDague=Tir
                PhaseJeuD=2
            end if
            'Deplacement Dague tirée
            if PhaseJeuD=2 then
                PosyD(TirageDague)++
                if PosyD(TirageDague)>=255+DecalVersion then 
                    PosyD(TirageDague)=255+DecalVersion
                    if PosXD(TirageDague)<=CX then 
                        PosXD(TirageDague)++
                        if PosXD(TirageDague)>=CX then PosXD(TirageDague)=CX:PhaseJeuD=3
                    end if
                    if PosXD(TirageDague)>CX then 
                        PosXD(TirageDague)--
                        if PosXD(TirageDague)<=CX then PosXD(TirageDague)=CX:PhaseJeuD=3
                    end if
                end if
            end if
            'Animation de la dague
            if PhaseJeuD=4 then
                GoAnimation=1
            end if
            
            'Fin du cycle
            if PhaseJeuD=5 then
                if PosXD(id)>=25 then PosXD(id)=PosXD(id)-4
                if PosXD(6)<=25 then PhaseJeuD=6
                if Tir=6 then PosXD(TirageDague)=25
            end if
            
            'NouveauCycle
            if PhaseJeuD=6 then
                for i=1 to 6
            	    PosyD(i)=184+DecalVersion
            	    PosXD(i)=25
                next  
            	CXDe=1 'Max=184
            	TempoVitesse=0
            	Velocite=0
            	Vitesse=19
            	aff=0
            	PrintText16 17,18,"  ",1
                PhaseJeuD=99 
                Ad=0
            	for i=1 to 6
            	    PosyD(i)=184
            	    PosXD(i)=25
            	    TempoDague(i)=0
            	    FrameID(i)=0
            	    loadtiles DagueSprite1,16,AdresseDague+(ad<<4)
            	    propsprite S_DagueD(i),AdresseDague+priority(1)+(ad<<4),3
            	    Ad++
            	next
            	Ad=0
            	if Tir=6 then
            	    if IDP=0 then WinGame=0 else WinGame=1
            	    GoAction=0
            	    PhaseJeuD=0
            	    IDP=0
                	DRAWTILESINC2 AdresseTileCombat+Palette(3)+priority(0),19+8,20+decalY,4,4
                	exit do
            	end if
            end if
            'Animation
            if GoAnimation then
                if Tir<6 then
                    TempoDague(TirageDague)++
                    if TempoDague(TirageDague)>5 then
                        FrameID(TirageDague)++
                        if FrameID(TirageDague)>5 then FrameID(TirageDague)=1:PhaseJeuD=5:GoAnimation=0:TempoDague(TirageDague)=0:FrameID(TirageDague)=1:Sleep 50
                        if FrameID(TirageDague)=1 then loadtiles DagueSprite1,16,AdresseDague+((TirageDague-1)<<4):gosub DagueOK
                        if FrameID(TirageDague)=2 then loadtiles DagueSprite2,16,AdresseDague+((TirageDague-1)<<4)
                        if FrameID(TirageDague)=3 then loadtiles DagueSprite3,16,AdresseDague+((TirageDague-1)<<4)
                        if FrameID(TirageDague)=4 then loadtiles DagueSprite4,16,AdresseDague+((TirageDague-1)<<4)
                        if FrameID(TirageDague)=5 then loadtiles DagueSprite5,16,AdresseDague+((TirageDague-1)<<4)
                        TempoDague(TirageDague)=0
                    end if
                else if Tir=6 then
                    TempoDague(TirageDague)++
                    if TempoDague(TirageDague)>5 then
                        FrameID(TirageDague)++
                        if FrameID(TirageDague)>5 then 
                            gosub MortSound
                            FrameID(TirageDague)=1
                            PhaseJeuD=50
                            GoAnimation=0
                            TempoDague(TirageDague)=0
                            FrameID(TirageDague)=1
                            propsprite S_DagueD(TirageDague),AdresseDague+priority(1)+((TirageDague-1)<<4),0
                            if IDP=0 then PrinttextCombat 6,24+decalY,"VOUS ETES MORT          "
                            if IDP=1 then
                	            if IdJoueur=1 then PrinttextCombat 6,24+decalY,"HANZO EST MORT          "
            	                if IdJoueur=2 then PrinttextCombat 6,24+decalY,"DILZUG EST MORT         "
            	                if IdJoueur=3 then PrinttextCombat 6,24+decalY,"MERLAG EST MORT         "
            	                if IdJoueur=4 then PrinttextCombat 6,24+decalY,"KRAYG EST MORT          "
                            end if
                        end if
                        if FrameID(TirageDague)=1 then loadtiles DagueSprite1,16,AdresseDague+((TirageDague-1)<<4):PosyD(TirageDague)=PosyD(TirageDague)-3::gosub DagueNO
                        if FrameID(TirageDague)=2 then loadtiles DagueSprite1,16,AdresseDague+((TirageDague-1)<<4):PosyD(TirageDague)=PosyD(TirageDague)-3
                        if FrameID(TirageDague)=3 then loadtiles DagueSprite1,16,AdresseDague+((TirageDague-1)<<4):PosyD(TirageDague)=PosyD(TirageDague)-3
                        if FrameID(TirageDague)=4 then loadtiles DagueSprite1,16,AdresseDague+((TirageDague-1)<<4):PosyD(TirageDague)=PosyD(TirageDague)-3
                        if FrameID(TirageDague)=5 then loadtiles DagueSprite1,16,AdresseDague+((TirageDague-1)<<4):PosyD(TirageDague)=PosyD(TirageDague)-3
                        TempoDague(TirageDague)=0
                    end if
                end if
            end if
            movesprite S_DagueD(id),PosXD(id),PosyD(id)
        next
		movesprite SpriteDe,CXDe,CYDe
        Sleep 1,TVBLANK
    loop
    
	freeallsprites
	stopplay 0
	SituationSound=1
	
	if WinGame=0 then 
	    EnduranceJoueur=0
	    LinkPage(Page)=666
	else 
	    LinkPage(Page)=36513
	end if
	gosub dynamicpage
    
    return
    

