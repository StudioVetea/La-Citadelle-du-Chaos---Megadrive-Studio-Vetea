		        PhaseJeu=PhaseJeuM+1
		        if PhaseJeu=7 then 
		            PhaseJeu=5
		        end if
		        if PhaseJeu=3 then gosub SortsSound
		    end if
		end if
		
		'Lors du lancer de dé, on OFF le PAD.
		if Goaction>1 then Joy=0

		'Gauche
		if joy.2 and tempo>2 and Phasejeu=0 and GoAction=0 then
			if PhaseJeu=0 then Gosub GantSound
			Tempo=0
			InfoTexte=0
			DX=12
			loadtiles EpeeCombat1S,16,AdresseTileCombat
			loadtiles FuiteCombatS,16,AdresseTileFuite
			if PhaseJeu=0 then PrinttextCombat 4,24+decalY,"COMMENCEZ LA PARTIE       "
