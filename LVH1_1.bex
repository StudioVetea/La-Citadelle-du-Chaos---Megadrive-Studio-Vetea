ModuleJeuDagueDingue:

    Dim S_DagueD(6) as integer
    Dim PosXD(6) as integer
    Dim PosYD(6) as integer
    Dim DagueD(6) as integer
    Dim TempoDague(6) as integer
    Dim FrameID(6) as integer
    Dim XD(6) as integer
    Dim IDJoueur as integer
    
    'Init
	DecalY=1
    Palettes PaletteEcranCombat,0,0,16
	Palettes PalettePointeur,3,0,16	
	Palettes PaletteDe,2,0,16

