	'Boucle principale
	do
	    gosub gestionsonore
		joy=joypad(0)
		if Ready!=0 then Joy=0
		if joy then randomize Randomseed
		Randomseed++
		if joy=0 then tempo++
		if tempo>5 then Tempo=5

		'Evénement combat ?
	    if ViandeCombat and CountCombat>0 then
	        if (TourJeu>=(CountCombat+1)) and Ready=0 then 
	            exit do  
	        end if  
	    end if 
		if CountCombat>0 and ViandeCombat=0 then
		    if SpecialCombat=0 then
        		if EventCombat and (TourJeu>=(CountCombat+1)) and Ready=0 then
        		    exit do
        		end if
            end if
		    if SpecialCombat=1 then
        		if EventCombat and (TourJeu>=(CountCombat+1)) and Ready=0 and TouchCombat=1 then
        		    freeallsprites
        		    exit do
        		end if
		    end if
		end if
		if SpecialCombat=0 and ViandeCombat=0 then
		    if EventCombat and TouchCombat and PAJoueur<PAMonstre and Ready=0 then GoStress=1:freeallsprites:exit do
		end if
		
		if ViandeCombat=0 then
		    if BLC=1 and GoChance=0 then
        		'Haut
        		if joy.0 and tempo>2 then
        			Choix++
        			Gosub GantSound
        			if Choix>2 then Choix=1
        			Tempo=0
        			aff=1
        			if GoFuite=1 then PrinttextCombat 5,24+decalY,"                            ":GoFuite=0:Tempo1=0
        		end if
        
        		'Bas
        		if joy.1 and tempo>2 then
        			Choix--
        			Gosub GantSound
        			if Choix=0 then Choix=2
        			'if Choix>255 then Choix=2
        			Tempo=0
        			aff=1
        			if GoFuite=1 then PrinttextCombat 5,24+decalY,"                            ":GoFuite=0:Tempo1=0
        		end if
        		
         		'On quitte ?
        		if choix=2 then GoFuite=1 else GoFuite=0
        
        		'On Valide la fuite !
        		if choix=2 and valid=1 and GoFuite=1 and tempo1>5 then 
        			freeallsprites
        			exit do
        		end if
		    end if
		    
    		'Haut
