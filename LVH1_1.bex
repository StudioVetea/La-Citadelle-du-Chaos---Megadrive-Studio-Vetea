'/////////////////////////////////////
'///// Module de Création du Héro ////
'/////////////////////////////////////
CreationHero:
	gosub CreatePointeur
	gosub CreateDe
	'Fading
	GoSub fadingZoomIN 
	'Option de d?lage texte EUR/US
	if decalversion>0 then DecalY=1
	'Init
	gosub initvariables
	if DecalY=0 then DeltaCY=8 else DeltaCY=0
	
	if versionlangue=0 then
    	printtext 4,3,"Cr#ation H#ros",1
    	printtext 2,6,"HABILETE  : 0",1
    	printtext 2,8,"Endurance : 0",1
    	printtext 2,10,"Chance    : 0",1
    	printtext 2,12,"Magie     : 0",1
	else
    	printtext 3,3,"Player Creation",1
    	printtext 2,6,"SKILL     : 0",1
    	printtext 2,8,"Stamina   : 0",1
    	printtext 2,10,"Luck      : 0",1
    	printtext 2,12,"Magic     : 0",1
	end if
	'printtext 2,14,"Quitter",1
	pointeurGantS 2,6+DecalY 

	if HabileteJoueur>0 then printtext 14,6,str$(HabileteJoueur),1
	if EnduranceJoueur>0 then printtext 14,8,str$(EnduranceJoueur),1
	if ChanceJoueur>0 then printtext 14,10,str$(ChanceJoueur),1
	if MagieJoueur>0 then printtext 14,12,str$(MagieJoueur),1
    
    if versionlangue=0 then
    	printText 21,6,"Lancer le D# !",1
    	printtext 21,8,"Essai 0 sur 3",1
	else
    	printText 21,6,"  Throw Dice !",1
    	printtext 21,8,"Try 0 of 3",1
	end if
 
	'Boucle principale
	do
        Gosub GestionSonore
		joy=joypad(0)
		RandomSeed++
		tempo++
		if joy=0 then tempo1++
		if Tempo1>2 then tempo1=2

		'Gestion Pad
		if valid=0 then
			if (tempo>10 and joy.1) then 
				Choix++
				Gosub GantSound
				if Choix>ChoixMax then choix=1
				tempo=0
				valid=0
				printtext 20,15,"                        ",1
				movesprite SpriteDe1,0,0
				CX=0
				aff=1
			end if
			
			if (tempo>10 and joy.0) then
				choix--
				Gosub GantSound
				if choix>255 then choix=0
				if choix=0 then choix=ChoixMax
				tempo=0
				valid=0
				printtext 20,15,"                        ",1
				movesprite SpriteDe1,0,0
				CX=0
				aff=1
			end if
		end if

		'Push Pointeur
		if (joy.7 or joy.6 or joy.5 or joy.4) then loadtiles PointeurGant1,16,AdressePointeur else loadtiles PointeurGant,16,AdressePointeur 

		'Lancer du d?
		if (tempo1>1 and (joy.7 or joy.6 or joy.5 or joy.4) and valid=0 and lancer=0) then 
			vitesse=0
			Gosub ValidationSound
			if choix=1 and essai<3 then o1=1:valid=1:vitesse=19
			if choix=2 and essai1<3 then o2=1:valid=1:vitesse=20
			if choix=3 and essai2<3 then o3=1:valid=1:vitesse=19
			if choix=4 and essai3<3 then o4=1:valid=1:vitesse=20
			if choix=5 then
				sleep 20
				exit do
			end if
			if vitesse>0 then
				CX=0
				tempo1=0
				CY=256
				printtext 20,15,"                        ",1
				movesprite SpriteDe1,0,CY
			end if
		end if

		if joy then randomize Randomseed

		'Gestion du (des) D?
		if lancer=0 and valid=1 then gosub LanceDe1

		'Pointeur & Choix
		if aff=1 then
		    if Versionlangue=0 then
    			printText 21,6,"Lancer le D# !  ",1
    			printtext 21,8,"Essai 0 sur 3",1
			else
            	printText 21,6,"  Throw Dice !",1
            	printtext 21,8,"Try 0 of 3",1
			end if
			printtext 18,12,"                  ",1
			if (habiletejoueur>0 and ChanceJoueur>0 and MagieJoueur>0 and EnduranceJoueur>0) then 
			    ChoixMax=5
			    if versionlangue=0 then printtext 2,14,"Quitter",1 else printtext 2,14,"Quit",1
			end if

			if choix=5 then
			    if versionlangue=0 then 
    				printtext 21,6,"               ",1
    				printText 21,6,"Quitter l'#cran",1
    				printtext 21,8,"de cr#ation.  ",1
    				printtext 2,6,"Habilet#  :",1
    				printtext 2,8,"Endurance :",1
    				printtext 2,10,"Chance    :",1
    				printtext 2,12,"Magie     :",1
    				printtext 2,14,"QUITTER",1
				else
    				printtext 21,6,"               ",1
    				printText 21,6,"Quit Player   ",1
    				printtext 21,8,"Creation.     ",1
    				printtext 2,6,"Skill     :",1
    				printtext 2,8,"Stamina   :",1
    				printtext 2,10,"Luck      :",1
    				printtext 2,12,"Magic     :",1
    				printtext 2,14,"QUIT  ",1
				end if
				pointeurGantS 2,14+DecalY
			end if

			if choix=1 then 
				'Tirage d? en cours ?
				if save(0)=1 then
					CX=save(1)
					if save(3)=0 then LoadTiles TileDe1,16,AdresseDe 
					if save(3)=1 then LoadTiles TileDe2,16,AdresseDe
					if save(3)=2 then LoadTiles TileDe3,16,AdresseDe
					if save(3)=3 then LoadTiles TileDe4,16,AdresseDe
					if save(3)=4 then LoadTiles TileDe5,16,AdresseDe
					if save(3)=5 then LoadTiles TileDe6,16,AdresseDe
					printtext 25,15,"+ 6 = "+str$(save(2))
				end if
				pointeurGantS 2,6+DecalY
				if versionlangue=0 then
    				printtext 2,6,"HABILETE  :",1
    				printtext 2,8,"Endurance :",1
    				printtext 2,10,"Chance    :",1
    				printtext 2,12,"Magie     :",1
				else
    				printtext 2,6,"Skill     :",1
    				printtext 2,8,"Stamina   :",1
    				printtext 2,10,"Luck      :",1
    				printtext 2,12,"Spell     :",1
				end if
				if essai>0 then 
					if versionlangue=0 then printtext 21,8,"Essai "+str$(essai)+" sur 3",1 else printtext 21,8,"Try "+str$(essai)+" of 3",1
				else
					if versionlangue=0 then printtext 21,8,"Essai 0 sur 3",1 else printtext 21,8,"Try 0 of 3",1
				end if
			end if
			
			if choix=2 then 
				'Tirage d? en cours ?
				if save1(0)=1 then
					CX=save1(1)
					if save1(3)=0 then LoadTiles TileDe1,16,AdresseDe 
