        				if Tirage=2 then LoadTiles TileDe3,16,AdresseDe
        				if Tirage=3 then LoadTiles TileDe4,16,AdresseDe
        				if Tirage=4 then LoadTiles TileDe5,16,AdresseDe
        				if Tirage=5 then LoadTiles TileDe6,16,AdresseDe
        				if Tirage1=0 then LoadTiles TileDe1,16,AdresseDe-16
        				if Tirage1=1 then LoadTiles TileDe2,16,AdresseDe-16
        				if Tirage1=2 then LoadTiles TileDe3,16,AdresseDe-16
        				if Tirage1=3 then LoadTiles TileDe4,16,AdresseDe-16
        				if Tirage1=4 then LoadTiles TileDe5,16,AdresseDe-16
        				if Tirage1=5 then LoadTiles TileDe6,16,AdresseDe-16
    				end if
    			end if
			
    			TempoVelocite++
    			if TempoVelocite>2 then 
    				Vitesse-=Velocite
    				if vitesse>255 then vitesse=0
    				CXDe+=Vitesse
    				TempoVelocite=0
    			end if
    			
    			'Fin du Tirage
    			if CXDe>210 then
    				CXDe=210
    				if PosJ>1 then
        				if Tirage=0 then LoadTiles DeMonstre_1,16,AdresseDe
        				if Tirage=1 then LoadTiles DeMonstre_2,16,AdresseDe
        				if Tirage=2 then LoadTiles DeMonstre_3,16,AdresseDe
        				if Tirage=3 then LoadTiles DeMonstre_4,16,AdresseDe
        				if Tirage=4 then LoadTiles DeMonstre_5,16,AdresseDe
        				if Tirage=5 then LoadTiles DeMonstre_6,16,AdresseDe
        				if Tirage1=0 then LoadTiles DeMonstre_1,16,AdresseDe-16
        				if Tirage1=1 then LoadTiles DeMonstre_2,16,AdresseDe-16
        				if Tirage1=2 then LoadTiles DeMonstre_3,16,AdresseDe-16
        				if Tirage1=3 then LoadTiles DeMonstre_4,16,AdresseDe-16
        				if Tirage1=4 then LoadTiles DeMonstre_5,16,AdresseDe-16
        				if Tirage1=5 then LoadTiles DeMonstre_6,16,AdresseDe-16
    				else
        				if Tirage=0 then LoadTiles TileDe1,16,AdresseDe 
        				if Tirage=1 then LoadTiles TileDe2,16,AdresseDe
        				if Tirage=2 then LoadTiles TileDe3,16,AdresseDe
        				if Tirage=3 then LoadTiles TileDe4,16,AdresseDe
        				if Tirage=4 then LoadTiles TileDe5,16,AdresseDe
        				if Tirage=5 then LoadTiles TileDe6,16,AdresseDe
        				if Tirage1=0 then LoadTiles TileDe1,16,AdresseDe-16
        				if Tirage1=1 then LoadTiles TileDe2,16,AdresseDe-16
        				if Tirage1=2 then LoadTiles TileDe3,16,AdresseDe-16
        				if Tirage1=3 then LoadTiles TileDe4,16,AdresseDe-16
        				if Tirage1=4 then LoadTiles TileDe5,16,AdresseDe-16
        				if Tirage1=5 then LoadTiles TileDe6,16,AdresseDe-16
    				end if
    				Tir=(Tirage+Tirage1)+2
    		        PhaseJeuM=PhaseJeu
    		        PhaseJeu=99
    		        PrintText16 11,18,str$(Tir),1
    		    end if
    	    end if
		end if
		
		'Résultat et relance
		if PhaseJeu=6 then 
		
		    'Bombe explose dans les mains du joueur ?
		    if Tir>Nombretour then
		        Gamers(PosJ)=1
		        if PosJ=1 then Movesprite SpriteJoueurCache(6),288,168
	            if PosJ=2 then Movesprite SpriteJoueurCache(5),344,192
	            if PosJ=3 then Movesprite SpriteJoueurCache(1),344,240
	            if PosJ=4 then Movesprite SpriteJoueurCache(2),288,264
	            if PosJ=5 then Movesprite SpriteJoueurCache(3),232,240
	            if PosJ=6 then Movesprite SpriteJoueurCache(4),232,192
	            NombreGamers--
	            'explosion
	            TempoRoc=0
	            Gosub ExplosionSound
	            
	            'Cas du héros ?
		        if PosJ=1 then 
		            Movesprite SpriteJoueurCache(6),288,168
		            HabileteJoueur=HabileteJoueur-2
		            EnduranceJoueur=EnduranceJoueur-4
		            'Mort ?
		            if EnduranceJoueur>255 then EnduranceJoueur=0:freeallsprites:goto Cred_666
    	            Do
            	        TempoRoc++
                	    if TempoRoc=1 then loadtiles RocBombeSpriteExplosion1,16,AdresseRocBombe
                	    if TempoRoc=5 then loadtiles RocBombeSpriteExplosion3,16,AdresseRocBombe
                	    if TempoRoc=10 then loadtiles RocBombeSpriteExplosion2,16,AdresseRocBombe
                	    if TempoRoc=15 then loadtiles RocBombeSpriteExplosion3,16,AdresseRocBombe
                	    if TempoRoc=20 then loadtiles RocBombeSpriteExplosion4,16,AdresseRocBombe
                	    if TempoRoc=25 then loadtiles RocBombeSpriteExplosion5,16,AdresseRocBombe
                	    if TempoRoc=30 then loadtiles RocBombeSpriteExplosion6,16,AdresseRocBombe
                	    if TempoRoc=35 then loadtiles RocBombeSpriteExplosion7,16,AdresseRocBombe
                	    if TempoRoc=40 then loadtiles RocBombeSpriteExplosion8,16,AdresseRocBombe
                	    if TempoRoc=45 then loadtiles RocBombeSpriteExplosion9,16,AdresseRocBombe
                	    if TempoRoc>=50 then 
                	        TempoRoc=0
                	        exit do
                	    end if
                	    Sleep 1,TVBLANK
            	    Loop
		            if orJoueur>=10 then printtextPage 5,2,str$(OrJoueur),1
		            if HabileteJoueur>=10 then printtextPage 6,4,str$(HabileteJoueur),1
		            if EnduranceJoueur>=10 then printtextPage 6,6,str$(EnduranceJoueur),1
		            if OrJoueur<10 then printtextPage 6,2,str$(OrJoueur),1:printtextPage 5,2,"0",1
                	if HabileteJoueur<10 then printtextPage 7,4,str$(HabileteJoueur),1:printtextPage 6,4,"0",1
                	if EnduranceJoueur<10 then printtextPage 7,6,str$(EnduranceJoueur),1:printtextPage 6,6,"0",1
                	PrinttextCombat 4,24+decalY,"VOUS AVEZ PERDU        "
    		        Movesprite SpriteJoueurCache(6),1,1
    		        Movesprite SpriteJoueurCache(5),1,1
    		        Movesprite SpriteJoueurCache(4),1,1
    		        Movesprite SpriteJoueurCache(3),1,1
    		        Movesprite SpriteJoueurCache(2),1,1
    		        Movesprite SpriteJoueurCache(1),1,1
                	for i=1 to 6
                	    Gamers(i)=0
                	next
        	        PrintText16 19,11,"  ",1
                	PrintText16 11,18,"  ",1
                	sleep 50
                	PhaseJeu=0
                	CxDe=1
                	TempoVitesse=0
                	Velocite=0
                	Vitesse=19
                	NombreTour=12
                	TempoRoc=0
                	GoAction=0
                	PosRocx=287
                	PosRocy=217
                	EnchantRoc=0
		            TempoPosJ=0
		            PosJ=1
		            DeltaTempoJ=0
		            CHJ=0
                	loadtiles RocBombeSprite5,16,AdresseRocBombe
                	Goto FinGame
		        end if
	            Do
        	        TempoRoc++
            	    if TempoRoc=1 then loadtiles RocBombeSpriteExplosion1,16,AdresseRocBombe
            	    if TempoRoc=5 then loadtiles RocBombeSpriteExplosion3,16,AdresseRocBombe
            	    if TempoRoc=10 then loadtiles RocBombeSpriteExplosion2,16,AdresseRocBombe
            	    if TempoRoc=15 then loadtiles RocBombeSpriteExplosion3,16,AdresseRocBombe
            	    if TempoRoc=20 then loadtiles RocBombeSpriteExplosion4,16,AdresseRocBombe
            	    if TempoRoc=25 then loadtiles RocBombeSpriteExplosion5,16,AdresseRocBombe
            	    if TempoRoc=30 then loadtiles RocBombeSpriteExplosion6,16,AdresseRocBombe
            	    if TempoRoc=35 then loadtiles RocBombeSpriteExplosion7,16,AdresseRocBombe
            	    if TempoRoc=40 then loadtiles RocBombeSpriteExplosion8,16,AdresseRocBombe
            	    if TempoRoc=45 then loadtiles RocBombeSpriteExplosion9,16,AdresseRocBombe
            	    if TempoRoc>=50 then 
            	        TempoRoc=0
            	        exit do
            	    end if
            	    Sleep 1,TVBLANK
        	    Loop
        	    
        	    'Victoire ?
        	    if NombreGamers=1 then
        	        gosub GongSound
        	        PrintText16 19,11,"  ",1
                	TempoVitesse=0
                	Velocite=0
                	Vitesse=19
                	NombreTour=12
                	CxDe=1
                	PrintText16 11,18,"  ",1
    		        Movesprite SpriteJoueurCache(6),1,1
    		        Movesprite SpriteJoueurCache(5),1,1
    		        Movesprite SpriteJoueurCache(4),1,1
    		        Movesprite SpriteJoueurCache(3),1,1
    		        Movesprite SpriteJoueurCache(2),1,1
    		        Movesprite SpriteJoueurCache(1),1,1
                	for i=1 to NombreJoueur
                	    if Gamers(i)=0 then PosJ=i
                	next
                	for i=1 to 6
                	    Gamers(i)=0
                	next
                	if PosJ!=1 then PrinttextCombat 4,24+decalY,"JOUEUR "+str$(PosJ)+" GAGNE   "
    		        if PosJ=1 then 
    		            PrinttextCombat 4,24+decalY,"VOUS AVEZ GAGNE              "
    		            OrJoueur=OrJoueur+36
    		            if OrJoueur>99 then OrJoueur=99
                    	printtextPage 5,2,"  ",1
                    	printtextPage 5,2,str$(OrJoueur),1
                    	printtextPage 6,4,str$(HabileteJoueur),1
                    	printtextPage 6,6,str$(EnduranceJoueur),1
                    end if
                	sleep 50
                	PhaseJeu=0
                	GoAction=0
                	PosRocx=287
                	PosRocy=217
                	EnchantRoc=0
		            TempoPosJ=0
		            PosJ=1
		            DeltaTempoJ=0
		            CHJ=0
                	loadtiles RocBombeSprite5,16,AdresseRocBombe
                	Goto FinGame
        	    end if
                PrinttextCombat 4,24+decalY,"JOUEUR "+str$(PosJ)+" EST ELIMINE   "
                Sleep 75
        	    PhaseJeu=3
    	        aff=0
    	        'NombreTour--
    	        'if NombreTour=0 then NombreTour=1
          	    NombreTour=12
    	        PrintText16 19,11,str$(NombreTour),1
            	TempoVitesse=0
            	Velocite=0
            	Vitesse=19
            	CxDe=1
            	PrintText16 11,18,"  ",1
        	    loadtiles RocBombeSprite5,16,AdresseRocBombe
        	    EnchantRoc=0
	            TempoPosJ=0
	            DeltaTempoJ=0
	            CHJ=0
        	    Goto FinGame
        	 end if
        	 
        	' Bombe explose si nombres égaux = Init
    	    if Tir=NombreTour then
	            'explosion
	            TempoRoc=0
	            PrinttextCombat 4,24+decalY,"RAZ PARTIE                "
	            Gosub ExplosionSound
	            Do
        	        TempoRoc++
            	    if TempoRoc=1 then loadtiles RocBombeSpriteExplosion1,16,AdresseRocBombe
            	    if TempoRoc=5 then loadtiles RocBombeSpriteExplosion3,16,AdresseRocBombe
            	    if TempoRoc=10 then loadtiles RocBombeSpriteExplosion2,16,AdresseRocBombe
            	    if TempoRoc=15 then loadtiles RocBombeSpriteExplosion3,16,AdresseRocBombe
            	    if TempoRoc=20 then loadtiles RocBombeSpriteExplosion4,16,AdresseRocBombe
            	    if TempoRoc=25 then loadtiles RocBombeSpriteExplosion5,16,AdresseRocBombe
            	    if TempoRoc=30 then loadtiles RocBombeSpriteExplosion6,16,AdresseRocBombe
            	    if TempoRoc=35 then loadtiles RocBombeSpriteExplosion7,16,AdresseRocBombe
            	    if TempoRoc=40 then loadtiles RocBombeSpriteExplosion8,16,AdresseRocBombe
            	    if TempoRoc=45 then loadtiles RocBombeSpriteExplosion9,16,AdresseRocBombe
            	    if TempoRoc>=50 then 
            	        TempoRoc=0
            	        exit do
            	    end if
            	    Sleep 1,TVBLANK
        	    Loop
    	        aff=0
            	TempoVitesse=0
            	Velocite=0
            	Vitesse=19
            	CxDe=1
            	NombreTour=12
		        Movesprite SpriteJoueurCache(6),1,1
		        Movesprite SpriteJoueurCache(5),1,1
		        Movesprite SpriteJoueurCache(4),1,1
		        Movesprite SpriteJoueurCache(3),1,1
		        Movesprite SpriteJoueurCache(2),1,1
		        Movesprite SpriteJoueurCache(1),1,1
            	for i=1 to 6
            	    Gamers(i)=0
            	next
            	PrintText16 19,11,"  ",1
            	PrintText16 11,18,"  ",1
            	sleep 50
            	PhaseJeu=0
            	GoAction=0
            	Raz=1
            	PosRocx=287
            	PosRocy=217
            	EnchantRoc=0
	            TempoPosJ=0
	            PosJ=1
	            DeltaTempoJ=0
	            CHJ=0
            	loadtiles RocBombeSprite5,16,AdresseRocBombe
        	    Goto FinGame
		    end if
		    
		    'Incrémentation
		    do
		        PosJ++
		        if PosJ>NombreJoueur then PosJ=1
		        if Gamers(PosJ)=0 then exit do
		    loop
		    
		    PosRocx=PosJoueurX(PosJ)
		    PosRocy=PosJoueurY(PosJ)
	        PhaseJeuM=PhaseJeu
	        aff=0
	        NombreTour--
	        PrintText16 19,11,str$(NombreTour),1
	        PhaseJeu=99
        	TempoVitesse=0
        	Velocite=0
        	Vitesse=19
        	CxDe=1
        	PrintText16 11,18,"  ",1
		end if
		
		
		'Validation pointeur
		'Début de partie
		if (joy.6 or joy.5 or joy.4) and tempo>2 and PhaseJeu=0 and DX=12 and Orjoueur>=3 and Raz=0 then 
		    PhaseJeu=1
	        OrJoueur=OrJoueur-3
            if orJoueur>=10 then printtextPage 5,2,str$(OrJoueur),1
            if OrJoueur<10 then printtextPage 6,2,str$(OrJoueur),1:printtextPage 5,2,"0",1
		    gosub GongSound
		    Tempo=0
		end if
		
		if (joy.6 or joy.5 or joy.4) and tempo>2 and PhaseJeu=0 and DX=12 and Raz=1 then 
		    PhaseJeu=1
		    gosub GongSound
		    Tempo=0
		end if
		
		
		if (joy.6 or joy.5 or joy.4) and tempo>2 and PhaseJeu=0 and DX=12 and Orjoueur<3 and Raz=0 then 
		    PrinttextCombat 4,24+decalY,"PAS ASSEZ D'OR              "
		    gosub ValidationSound
		    Tempo=0
		end if
		
		'On quitte ?
		if (joy.6 or joy.5 or joy.4) and tempo>2 and PhaseJeu=0 and DX=15 then 
		    gosub Validationsound
		    Tempo=0
		    exit do
		end if
		
		'Pointeur
		if Ready=0 and Goaction=0 then 
		    pointeurGantS 3+(DX<<1),22+DecalY-(DY<<2)
		end if
		if Goaction=1 then movesprite PointeurS,1,1
		
		'Push Pointeur
		if (joy.6 or joy.5 or joy.4) then loadtiles PointeurGant1,16,AdressePointeurCombat else loadtiles PointeurGant,16,AdressePointeurCombat 
	    
	    'Animation Sprites
	    'Roc Enchanté
	    if EnchantRoc then
	        TempoRoc++
    	    if TempoRoc=1 then loadtiles RocBombeSprite1,16,AdresseRocBombe
    	    if TempoRoc=5 then loadtiles RocBombeSprite2,16,AdresseRocBombe
    	    if TempoRoc=10 then loadtiles RocBombeSprite3,16,AdresseRocBombe
    	    if TempoRoc=15 then loadtiles RocBombeSprite4,16,AdresseRocBombe
    	    if TempoRoc>=20 then TempoRoc=0
	    end if
	    
FinGame:
	    'Affichage Sprites
	    MoveSprite SpriteRoc,PosRocx,PosRocY
		movesprite SpriteDe,CXDe,CYDe
		movesprite SpriteDe1,CXDe+32,CYDe
	    Sleep 1,TVBLANK
	loop
	
	freeallsprites
	stopplay 0
	SituationSound=1
	
	'Retour au paragraphe.
	LinkPage(Page)=38511
	gosub dynamicpage
	
    return 
    
