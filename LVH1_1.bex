    PropSprite spDague1,1100,1
    MoveSprite spDague1,309,181
    
    Global spDague2 As Integer
    spDague2=AddSprite(3,4)
    PropSprite spDague2,1112,1
    MoveSprite spDague2,309,205
    '========================================='
    '========================================='
    '                 LAYER A                 '
    '========================================='
    '========================================='
    
    SetGfxPlane Scroll_A
    
    Reload tilemap_LANGUES_LAYER_A
    
    For y = 0 To 27
    	For x = 0 To 39
    		ReadInt numeroTile
    		tilemap_SCENE(x,y) = numeroTile
    	Next x
    Next y
    	
    For y = 0 To 27
    	For x = 0 To 39
    		DrawTile tilemap_SCENE(x,y)+865+Palette(1),x,y
    	Next x
    Next y	
    
    '========================================='
    '========================================='
    '                 LAYER B                 '
    '========================================='
    '========================================='
    
    SetGfxPlane Scroll_B
    
    Reload tilemap_LANGUES_LAYER_B
    
    For y = 0 To 27
    	For x = 0 To 39
    		ReadInt numeroTile
    		tilemap_SCENE(x,y) = numeroTile
    	Next x
    Next y
    	
    For y = 0 To 27
    	For x = 0 To 39
    		DrawTile tilemap_SCENE(x,y)+256+Palette(0),x,y
    	Next x
    Next y	
    
    enable screen
    fadeinEx lblptr&(pal_ECRAN_LANGUES),0,5,3
    t=0
    Versionlangue=1
    D=0
    G=0
    do
        j=joypad(0)
        if j=0 then t++
        if t>3 then t=3
        if t>2 then
            if (J.4 [or] J.6 [or] J.5 [or] J.7) then gosub Gantsound:t=0:exit do
            if j.2 then G=1:D=0:t=0:gosub Gantsound
            if J.3 then D=1:G=0:t=0:gosub Gantsound
            if D then   
                MoveSprite spDague1,309,181
                MoveSprite spDague2,309,205
                Versionlangue=1
            end if
            if G then
                MoveSprite spDague1,262,181+36
                MoveSprite spDague2,262,205+36
                Versionlangue=0
            end if
        end if
        Sleep 1,TVBLANK
    loop
    
     fadeoutEx lblptr&(pal_ECRAN_LANGUES),0,5,3
	'Init Ecran Plan A
	 GoSub BlackScreen
	 If decalversion=0 Then Scroll2 Up,0 Else Scroll2 Up,-8 
	 Disable Screen
	 freeallsprites
	 stopplay 0
     return

'//////////////////////////
'///// Init G?ral  /////
'/////////////////////////
Init:
	'Version de MD ?
	If TvType() Then version=1 Else version=0
	If version=1 Then DecalVersion=8
	If decalversion>0 Then GoSub EurPlanB
	 
	'Pour les gros Blocs de Textes > 30 Tile, on utilise DecalYXY que l'on soustrait ?a longeur du texte ( En mode EUR )
	If decalversion>0 Then 
		DecalY=2 
		DecalYXL=1
		DecalText=0
	Else 
		DecalY=0
		DecalYXL=0
		DecalText=3
	End If
	 
	'Reset de la priorit?ext / Sprite
	'Option TextPriority,High
	Option NoLoadFont
	LoadTiles FontTile,100,33
	SetScrollPlane Scroll_B
	SetTextPlane Scroll_A
	SetScrollMode HScroll_OverAll, VScroll_OverAll
	AdresseFading=1343
	'Pointeur Gant
	AdressePointeur=1343-16
	AdresseDe=AdressePointeur-24-16
	Scenario=0
	TouchCombat=0
	SpecialCombat=0
	CountCombat=0
	GoSub FormulemagiquesTextes
	'Inventaire
	if VersionLangue then Inventaire$(0)="Sword" else Inventaire$(0)="Ep#e"
        
	return
	
	

'/////////////////////////////////////
'/////  Cr?ion pointeur Gants  /////
'/////////////////////////////////////
CreatePointeur:
	Palettes PalettePointeur,3,0,16
	loadtiles PointeurGant,16,AdressePointeur
	PointeurS=addsprite(4,4)
	propsprite PointeurS,AdressePointeur+priority(1),3
	'Au dela de la 1ere Page, on cr? un deuxi? Gant pour surfer entre les pages
	if Page>0 then
		loadtiles PointeurGant,16,AdressePointeur-16
		PointeurS1=addsprite(4,4)
		propsprite PointeurS1,AdressePointeur-16+priority(1),3
	end if
	return
	
	

'/////////////////////////////////////
'/////   Cr?ion Sprite D?    /////
'/////////////////////////////////////
CreateDe:
	Palettes PaletteDe,2,0,16
	LoadTiles TileDe1,16,AdresseDe 
	SpriteDe=AddSprite(4,4)
	SpriteDe1=AddSprite(4,4)
	PropSprite SpriteDe,AdresseDe+Priority(1),2
	PropSprite SpriteDe1,AdresseDe+Priority(1)-16,2
