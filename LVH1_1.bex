			aff=1
			Pik=0
			Tems=0
			TemK=0
			Chance=0
			vitesse=19 
			'NombreMonstre=2
			Nombre=0
			
			if PorteCombat=0 then
    			for i=1 to NombreMonstre
    				if EnduranceM(i)>0 then 
    					Nombre++
    					u=i
    				else
    					if versionlangue=0 then 
