		        PosJ++
		        DeltaTempoJ++
		        CHJ++
		        TempoPosJ=0
		        gosub ValidationSound
		        if PosJ>NombreJoueur then PosJ=1
		        do
		            if Gamers(PosJ)=1 then PosJ++
		            if PosJ>NombreJoueur then PosJ=1
