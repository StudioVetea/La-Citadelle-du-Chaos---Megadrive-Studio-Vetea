    			Choix++
    			Gosub GantSound
    			if Choix>2 then Choix=0
    			Tempo=0
    			aff=1
    			if GoFuite=1 then PrinttextCombat 5,24+decalY,"                            ":GoFuite=0:Tempo1=0
    		end if
    
    		'Bas
    		if joy.1 and tempo>2 and ChancePage=0  then
    			Choix--
    			Gosub GantSound
    			if Choix>255 then Choix=2
    			Tempo=0
    			aff=1
