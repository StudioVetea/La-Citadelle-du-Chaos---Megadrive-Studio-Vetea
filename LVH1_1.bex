		        Movesprite SpriteJoueurCache(4),232,192
		        Movesprite SpriteJoueurCache(3),232,240
		        Movesprite SpriteJoueurCache(2),288,264
		    end if
		    if NombreJoueur=2 then
		        Movesprite SpriteJoueurCache(4),232,192
		        Movesprite SpriteJoueurCache(3),232,240
		        Movesprite SpriteJoueurCache(2),288,264
		        Movesprite SpriteJoueurCache(1),344,240
		        'Movesprite SpriteJoueurCache(5),344,192
		    end if
		    PhaseJeuM=PhaseJeu
		    PhaseJeu=99
		end if
		
		'Enchantement Rocher
		if phasejeu=3 then
		    EnchantRoc=1
		    PrinttextCombat 4,24+decalY,"ENCHANTEMENT ROCHER         "
		    PhaseJeuM=PhaseJeu
		    PhaseJEu=99
		    TirJ=rnd(50)
		    CHJ=0
		end if
		
		'Choix Joueur
		if PhaseJeu=4 then
		    PrinttextCombat 4,24+decalY,"CHOIX JOUEUR                 "
		    TempoPosJ++
