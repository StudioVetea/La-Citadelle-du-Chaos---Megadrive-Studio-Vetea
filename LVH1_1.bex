            		PrintText16 11,18+DecalY,str$(AllyHAB),1
            		PrintText16 4,7+DecalY,str$(AllyEND),1
			    else
                    if versionlangue=0 then PrinttextCombat 8,5+DecalY+B,"VOUS     " else  PrinttextCombat 8,5+DecalY+B,"YOU      "
            		PrintText16 12,7+DecalY,str$(HabileteJoueur),1
            		PrintText16 11,18+DecalY,str$(HabileteJoueur),1
            		PrintText16 4,7+DecalY,str$(EnduranceJoueur),1
			    end if
			end if
			
			
			CXDeChance=1
			CXDeJoueur=1
			CXDeMonstre=456
			PrinttextCombat 7,24+decalY,"                         "
			PrintText16 28,21+DecalY,"  ",1
			PrintText16 23,18+DecalY,"  ",1
			PrintText16 5,18+DecalY,"  ",1
			PrintText16 10,21+DecalY,"  ",1
			
			if PorteCombat=0 and viandeCombat=0 then
    			if Nombre>0 then 
    				GoCombat=1
    				TourJeu++
    			end if
			end if
		end if

		'Victoire ?
		if GoCombat=5 then
			temo++
			CombatPage=2
			if temo<10 then 
			    if versionlangue=0 then 
			        PrintTextCombat 14,3+DecalY,"VICTOIRE",1 else PrintTextCombat 14,3+DecalY,"        ",1
			    else
			        PrintTextCombat 14,3+DecalY,"VICTORY ",1 else PrintTextCombat 14,3+DecalY,"        ",1
			    end if
			end if
			if temo>20 then temo=0:temA++
		end if

		if GoFuite=1 then
			if Joy=0 then Tempo1++
			if versionlangue=0 then 
			    PrinttextCombat 5,24+decalY,"APPUYEZ A NOUVEAU POUR FUIR"
			else
			    PrinttextCombat 5,24+decalY,"PRESS AGAIN TO GIVE UP     "
			end if
			CombatPage=3
		end if

		'The End ... :(
		if GoCombat=6 then
			temo++
			if versionlangue=0 then 
			    if temo<5 then PrintTextCombat 4,24+DecalY,"         VOUS ETES MORT         ",1 else PrintTextCombat 4,24+DecalY,"                              ",1
			else
			    if temo<5 then PrintTextCombat 4,24+DecalY,"          YOU ARE DEAD          ",1 else PrintTextCombat 4,24+DecalY,"                              ",1
			end if
			if temo>10 then temo=0:temA++
			GoFuite=1
			GoMort=1
		end if

		'Partie Gagn?
		if temA>6 then exit do

		'Gestion des phases de jeu
		'Gestion des Combats
		'Lancer Joueur
		if GoCombat=1 then
			TempoVitesse++
			if Velocite=0 and TempoVitesse=1 then Gosub DeSound
			RandomSeed++
			if joy then randomize Randomseed
			Tirage=Rnd(5)
			Ti1=Rnd(10)
			If Ti1>5 And Tirage=4 Then Tirage=5
			RandomSeed++
			if joy then randomize Randomseed
			Tirage1=Rnd(5)
			Ti11=Rnd(10)
			If Ti11>5 And Tirage1=4 Then Tirage1=5
			Tir=(Tirage+Tirage1)+2
			if PorteCombat>0 then Tir=Tirage+1
			
			if TempoVitesse>25 then 
				Velocite++
				TempoVitesse=0
				if Tirage=0 then LoadTiles TileDe1,16,AdresseDe 
				if Tirage=1 then LoadTiles TileDe2,16,AdresseDe
				if Tirage=2 then LoadTiles TileDe3,16,AdresseDe
				if Tirage=3 then LoadTiles TileDe4,16,AdresseDe
				if Tirage=4 then LoadTiles TileDe5,16,AdresseDe
				if Tirage=5 then LoadTiles TileDe6,16,AdresseDe
				if Tirage1=0 then LoadTiles TileDe1,16,AdresseDe-16
				if Tirage1=1 then LoadTiles TileDe2,16,AdresseDe-16
				if Tirage1=2 then LoadTiles TileDe3,16,AdresseDe-16
				if Tirage1=3 then LoadTiles TileDe4,16,AdresseDe-16
				if Tirage1=4 then LoadTiles TileDe5,16,AdresseDe-16
				if Tirage1=5 then LoadTiles TileDe6,16,AdresseDe-16
			end if
			
			TempoVelocite++
			if TempoVelocite>2 then 
				Vitesse-=Velocite
				if vitesse>255 then vitesse=0
				CXDeJoueur+=Vitesse
				TempoVelocite=0
			end if
			
			'Fin du Tirage
			if CXDeJoueur>184 then
				if Tirage=0 then LoadTiles TileDe1,16,AdresseDe 
				if Tirage=1 then LoadTiles TileDe2,16,AdresseDe
				if Tirage=2 then LoadTiles TileDe3,16,AdresseDe
				if Tirage=3 then LoadTiles TileDe4,16,AdresseDe
				if Tirage=4 then LoadTiles TileDe5,16,AdresseDe
				if Tirage=5 then LoadTiles TileDe6,16,AdresseDe
				if Tirage1=0 then LoadTiles TileDe1,16,AdresseDe-16
				if Tirage1=1 then LoadTiles TileDe2,16,AdresseDe-16
				if Tirage1=2 then LoadTiles TileDe3,16,AdresseDe-16
				if Tirage1=3 then LoadTiles TileDe4,16,AdresseDe-16
				if Tirage1=4 then LoadTiles TileDe5,16,AdresseDe-16
				if Tirage1=5 then LoadTiles TileDe6,16,AdresseDe-16
				PrintText16 5,18+DecalY,str$(Tir),1
				PAJoueur=Tir+HabileteJoueur+BonusForce+BonusFA
				'Mode Ally ?
				if AllyCombat then 
				    If AllyEnd=0 then AllyMort=1
				    if AllyMort=0 then
    				    if TourJeu%2=1 then 
    				        PAJoueur=Tir+HabileteJoueur
    				    elseif TourJeu%2=0 then
    				        PAJoueur=Tir+AllyHAB
