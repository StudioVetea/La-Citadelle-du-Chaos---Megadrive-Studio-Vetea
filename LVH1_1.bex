    					else
    					    if EnduranceM(i)=0 then Monstre$(i)="DEAD MONSTER           "
    					end if
    				end if
    			next
			
    			if Nombre=1 then TourMonstre=u
    			TourM=TourMonstre
    			
    			if Nombre>1 and TourJeu>1 then 
    				TourMonstre++
    				if TourMonstre>NombreMonstre then TourMonstre=1
    			end if
			end if
			
			'Mode Ally ?
			if AllyCombat then
			    if TourJeu%2=0 and AllyMort=0 then
			        if versionlangue=0 then PrinttextCombat 8,5+DecalY+B,"LE PETIT" else PrinttextCombat 8,5+DecalY+B,"THE SMALLER"
