			Velocite=0
			CXDegat=1 'MAx=168
			CXDeJoueur=1 'Max=184
			CXDeMonstre=456 'Max = 184+134=318
			CXDeChance=1 'Max=208
			aff=1
			Tems=0
			TemK=0
			Chance=0
			vitesse=19 
			'NombreMonstre=2
			Nombre=0
