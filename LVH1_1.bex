'///////   Module Jeu Pique-Six        /////
'///////////////////////////////////////////
ModuleJeuPiqueSix:
    Dim MiseJeu(6) as integer
    Dim ChiffreJeu(6) as integer
	gosub BookOuvert
	gosub ScreenPiqueSix_A
	'Init
	DecalY=1
	Palettes PaletteEcranCombat,0,0,16
	Palettes PalettePointeur,3,0,16
	
	'Adressage Tiles/Sprites
	AdresseChiffre=(AdrBookOuvert+135+39)
	AdresseTileCombat=AdresseChiffre+48
	AdresseTileChance=AdresseTileCombat+16
	AdresseTileFuite=AdresseTileChance+16
	AdresseSpriteDegat=AdresseTileFuite+16
	AdresseFontCombat=AdresseSpriteDegat+16
	AdressePointeurCombat=AdresseDe-112
	
	'Chargement VRAM
	Palettes PaletteDe,2,0,16
	LoadTiles TileDe1,16,AdresseDe 
	LoadTiles TileDe1,16,AdresseDe-16
	LoadTiles DeMonstre_1,16,AdresseDe-32
	LoadTiles DeMonstre_1,16,AdresseDe-64
	LoadTiles TileDe1,16,AdresseDe-80
	LoadTiles TileDe1,16,AdresseDe-96
	loadtiles Chiffre16,48,AdresseChiffre
	loadtiles EpeeCombat1S,16,AdresseTileCombat
	loadtiles ChanceCombatS,16,AdresseTileChance
	loadtiles FuiteCombatS,16,AdresseTileFuite
	loadtiles Degat_1S,16,AdresseSpriteDegat
	loadtiles FontCombat,36,AdresseFontCombat
	loadtiles PointeurGant,16,AdressePointeurCombat
	
	'Sprites
	SpriteDe=AddSprite(4,4)
	PointeurS=addsprite(4,4)
	PropSprite SpriteDe,AdresseDe+Priority(1),2
	propsprite PointeurS,AdressePointeurCombat+priority(1),3
	
	'Affichage Icone
	DRAWTILESINC2 AdresseTileCombat+Palette(3)+priority(0),14,20+decalY,4,4
	DRAWTILESINC2 AdresseTileFuite+Palette(3)+priority(0),22,20+decalY,4,4
	
	'Init
	if OrJeu=0 then OrJeu=OrJoueur
    stopplay 0
    SituationSound=2
    DY=0
    DX=6
    Mise=0
    Pari=0
    MIX=3
    CHIX=3
    GoAction=0
    Tir=0
	tempo=0
	TemA=0
	TemK=0
	TemG=0
	Valid=0
	Tempo1=0
	TempoVitesse=0
	Velocite=0
	vitesse=19
	Ready=0
	CYDe=264+DecalVersion
	CXDeJoueur=1 'Max=184
	InfoTexte=0
	
	'Ecriture données
	if OrJeu>9 then PrintText16 19,2+DecalY,str$(OrJeu),1 else PrintText16J 19,2+DecalY,str$(OrJeu),1
	for i=1 to 6
	    MiseJeu(i)=0
	    ChiffreJeu(i)=0
	next
	
	
	'Boucle principale
	do
	    gosub gestionsonore
		joy=joypad(0)
		if Ready!=0 then Joy=0
		if joy then randomize Randomseed
		if joy=0 then tempo++
		if tempo>5 then Tempo=5
		
		'Lors du lancer de dé, on OFF le PAD.
		if Goaction>1 then Joy=0

		'Haut
		if joy.0 and tempo>2 then
		    if DY=2 then DY++
		    if DY=0 then DY=DY+2
			Gosub GantSound
			Tempo=0
			InfoTexte=0
		end if

		'Bas
		if joy.1 and tempo>2 then
			DY--
			if DY=1 then DY--
			if DY>255 then DY=0
			Gosub GantSound
			if DY=0 then DX=6:MIX=3:CHIX=3
			Tempo=0
			InfoTexte=0
		end if
		
		'Gauche
		if joy.2 and tempo>2 then
		    'Ligne du bas
		    if DY=0 then
		        DX=DX-4
		        if DX<6 then DX=6
		    end if
		    '1ere Ligne
		    if DY>0 then
		        MIX--
		        CHIX--
		        If MIX<1 then MIX=1
		        if CHIX>255 then CHIX=0
		        DX=DX-3
		        if DX>255 then DX=0
		    end if
			Gosub GantSound
			Tempo=0
			InfoTexte=0
		end if
		
		'Droite
		if joy.3 and tempo>2 then
		    if DY=0 then
		        DX=DX+3
		        if DX>9 then DX=9
		    end if
		    '1ere Ligne
		    if DY>0 then
		        DX=DX+3
		        if DX>15 then DX=15
		        MIX++
		        CHIX++
		        If MIX>6 then MIX=6
		        if CHIX>6 then MIX=6
		    end if
			Gosub GantSound
			Tempo=0
			InfoTexte=0
		end if
		
		'Info Texte
		if InfoTexte=0 then
		    if DY=0 and DX=6 then PrinttextCombat 7,24+decalY,"   LANCEZ VOTRE DE        "
	        if DY=0 and DX=9 then PrinttextCombat 7,24+decalY,"   QUITTEZ LA PARTIE      "
	        if DY=2 then PrinttextCombat 7,24+decalY,"   EFFECTUEZ VOTRE MISE          "
	        if DY=3 then PrinttextCombat 4,24+decalY,"   CHOISISSEZ VOTRE CHIFFRE          "
		    InfoTexte=1
		end if
		
		'On quitte ?
		If Valid=1 and DY=0 and DX=9 then
		    Valid=0
		    exit do
		end if
		
		If Valid=1 and DY=0 and DX=6 and Goaction=0 then
		    Valid=0
		end if
		
		'Lancer de dé
		if Valid=1 and GoAction=1 and DY=0 and DX=6  then
		    Gosub DeSound
		    GoAction=2
		    Valid=0
		end if
		
		'On lance le dé ?
		if GoAction=2 then
			TempoVitesse++
			RandomSeed++
		    if joy then randomize Randomseed
		    Randomize RandomSeed
		    Tirage=Rnd(5)
			Ti1=Rnd(10)
			If Ti1>5 And Tirage=4 Then Tirage=5
			Tir=Tirage+1
			if Tirage=0 then LoadTiles TileDe1,16,AdresseDe 
			if Tirage=1 then LoadTiles TileDe2,16,AdresseDe
			if Tirage=2 then LoadTiles TileDe3,16,AdresseDe
			if Tirage=3 then LoadTiles TileDe4,16,AdresseDe
			if Tirage=4 then LoadTiles TileDe5,16,AdresseDe
			if Tirage=5 then LoadTiles TileDe6,16,AdresseDe
			
			if TempoVitesse>25 then 
				Velocite++
				TempoVitesse=0
			end if
			
			TempoVelocite++
			if TempoVelocite>2 then 
				Vitesse-=Velocite
				if vitesse>255 then vitesse=0
				CXDeJoueur+=Vitesse
				TempoVelocite=0
			end if
			
			'Fin du Tirage
			if CXDeJoueur>260 then
				if Tirage=0 then LoadTiles TileDe1,16,AdresseDe 
				if Tirage=1 then LoadTiles TileDe2,16,AdresseDe
				if Tirage=2 then LoadTiles TileDe3,16,AdresseDe
				if Tirage=3 then LoadTiles TileDe4,16,AdresseDe
				if Tirage=4 then LoadTiles TileDe5,16,AdresseDe
				if Tirage=5 then LoadTiles TileDe6,16,AdresseDe
				TempoVitesse=0
				Velocite=0
				vitesse=19
				tempo=0
				GoAction=3
			end if
		end if
		
		'Traitement du tirage
		if Goaction=3 then
		    TemA++
		    if TemA>75 then
		        'Raz
		        TemA=0
		        TemK++
		        CXDeJoueur=1
		        Goaction=0
            	for i=1 to 6 
            	    if i>1 then
                    	locate 8+DecalY,((i<<1)-3)+(i<<2):print "  "
                    	locate 9+DecalY,((i<<1)-3)+(i<<2):print "  "
                     	locate 12+DecalY,((i<<1)-3)+(i<<2):print "  "
                    	locate 13+DecalY,((i<<1)-3)+(i<<2):print "  "
                    else
                        locate 8+DecalY,2+(i):print "  "
                    	locate 9+DecalY,2+(i):print "  "
                        locate 12+DecalY,2+(i):print "  "
                    	locate 13+DecalY,2+(i):print "  "
                	end if
                	
                	if Tir=ChiffreJeu(i) and MiseJeu(i)>0 then
                	    OrJeu=OrJeu+(MiseJeu(i)*5)
                	    if OrJeu>99 then OrJeu=99
                	    gosub OrSound
	                    if OrJeu>9 then PrintText16 19,2+DecalY,str$(OrJeu),1 else PrintText16J 19,2+DecalY,str$(OrJeu),1
                	end if
            	next 
             	for i=1 to 6
            	    MiseJeu(i)=0
            	    ChiffreJeu(i)=0
            	next
            	CHIX=3
            	MIX=3
		    end if    
		end if
		
		'Gestion Données
		if Valid=1 then
		    'Mise
    		if DY=2 then
    		    if OrJeu!=0 then 
        		    Misejeu(MIX)++
        		    if Misejeu(MIX)>9 then Misejeu(MIX)=0:OrJeu=OrJeu+10
        		    if ChiffreJeu(CHIX)=0 then PrintText16J 3+(DX<<1),8+DecalY,str$(ChiffreJeu(CHIX)+1),1:ChiffreJeu(CHIX)=1
        		else if OrJeu=0 then
        		    OrJeu=OrJeu+Misejeu(MIX)+1
        		    Misejeu(MIX)=0   
        		end if
    		    OrJeu--
    		    GoAction=1
    		    if OrJeu>255 then OrJeu=0:GoAction=0
    		    PrintText16J 3+(DX<<1),12+DecalY,str$(Misejeu(MIX)),1
    		    'Or
    		    if OrJeu>=10 then 
    		        PrintText16 19,2+DecalY,str$(OrJeu),1 
    		    else 
    		        PrintText16J 19,2+DecalY,str$(OrJeu),1 
    		        locate 3,21:print "  "
    		        locate 4,21:print "  "
    		    end if
    		    Valid=0
    		end if
    		
    		'Chiffres Pari
    		if DY=3 then
    		    ChiffreJeu(CHIX)++
    		    if Misejeu(MIX)=0 then PrintText16J 3+(DX<<1),12+DecalY,str$(Misejeu(MIX)),1
    		    if ChiffreJeu(CHIX)>6 then ChiffreJeu(CHIX)=1
    		    PrintText16J 3+(DX<<1),8+DecalY,str$(ChiffreJeu(CHIX)),1
    		    Valid=0
    		end if
		end if
		
		if DY>3 then DY=3
		'Validation pointeur
		if (joy.6 or joy.5 or joy.4) and tempo>2 and valid=0 then valid=1:tempo=0:Gosub ValidationSound
		
		'Pointeur
		if Ready=0 then 
		    pointeurGantS 3+(DX<<1),22+DecalY-(DY<<2)
		end if
		'Push Pointeur
		if (joy.6 or joy.5 or joy.4) then loadtiles PointeurGant1,16,AdressePointeurCombat else loadtiles PointeurGant,16,AdressePointeurCombat 
        
        'Sprite
        movesprite SpriteDe,CXDeJoueur,CYDe
        
	    Sleep 1,TVBLANK
	loop
	
	'Or Gagné avant de partir
	if TemK>0 then
	    OrJoueur=OrJeu
	end if
	stopplay 0
	SituationSound=1
	
	'Retour au paragraphe.
	LinkPage(Page)=38511
	gosub dynamicpage
    return
	
	
 
