'//////////////////////////////////////
'/////// Module des Questions  ////////
'//////////////////////////////////////
ModuleQuestion:
	Gosub CreatePointeur
	if decalversion=0 then DecalY=0 else DecalY=1
	if TypePage$="G" then X=TexteX-1 else X=TexteX
	
	if Question$(0)="" then ST=1 else ST=0
	

	'Affichage des questions
	for i=ST to LQ
		if i>0 then 
			printtext x,TexteY+i+Lo+2,Question$(i),1 
		else
			printtext x,TexteY+i+Lo+2,QuestionG$(i),1 
		end if
	next

	'Boucle principale
	Tempo=0
	Choix=ST
	ChoixMax=LQ
	valid=0
	aff=0
	'Cas particulier
	if ST=1 then printtext x,TexteY+Choix+Lo+2,QuestionG$(Choix),1 
	
	do 
	    
        Gosub GestionSonore
		joy=joypad(0)
		'Changement Font
		if Joy.5 and FontChange=0 then gosub ChangeFont:FontChange=1
		if Joy=0 then FontChange=0
		if joy=0 then Tempo++
		'Gestion Pad
		if valid=0 then
			if (tempo>10 and joy.1) then 
				Choix++
				Gosub GantSound
				if Choix>ChoixMax then choix=ST
				tempo=0
				valid=0
				CX=0
				aff=1
			end if
			if (tempo>10 and joy.0) then
				choix--
				Gosub GantSound
				if ST=1 and Choix=0 then choix=ChoixMax
				if choix>255 then choix=ChoixMax
				tempo=0
				valid=0
				CX=0
				aff=1
			end if
		end if

		'Push Pointeur
		if (joy.6 or joy.4) then loadtiles PointeurGant1,16,AdressePointeur else loadtiles PointeurGant,16,AdressePointeur 
		PointeurGantS x,TexteY+Lo+2+Choix+DecalY

		'Feuille Aventure ?
		if joy.7 then gosub GestionFeuille

		if aff=1 then
			'Affichage des questions
			for i=ST to LQ
				if i=choix then 
					printtext x,TexteY+i+Lo+2,QuestionG$(i),1 
				else
					printtext x,TexteY+i+Lo+2,Question$(i),1 
				end if
			next
		end if

		'Choix !
		if (joy.6 or joy.4) and tempo>10 then 
		    gosub ValidationSound
		    exit do
		end if

		aff=0
		sleep 1,TVBlank
	loop
	
	freeallsprites
	'RAZ Question
	for i=ST to LQ
		printtext x,TexteY+i+Lo+2,"                                    ",1
	next
	
	'Attribution de la prochaine destination.
	LinkPage(Page+1)=PageLiaison(choix)
	
	'M?risation du choix de la page.
	IsQuestionPage(Page)=choix+1
	UseQuestion(Page)=2
	ReadPage(Page)=1
	gosub DynamicPage
	return
 
 
 
 
 

