	DRAWTILESINC2 AdresseTileFuite+Palette(3)+priority(0),25+8,20+decalY,4,4
	PrinttextCombat 4,24+decalY,"COMMENCEZ LA PARTIE       "
    
    'Boucle principale
	do
	    gosub gestionsonore
	    
	    'Gestion PAD
		joy=joypad(0)
		Randomseed++
		if Ready!=0 then Joy=0
		if joy then randomize Randomseed
		if joy=0 then tempo++
		if tempo>5 then Tempo=5
		
		'Gestion Tempo Jeu
