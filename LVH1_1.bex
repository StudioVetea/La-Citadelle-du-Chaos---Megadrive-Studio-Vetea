'//////////////////////////////////////////////
'///// Proc?re Texte en 16x16 - 2 digits/////
'//////////////////////////////////////////////
Declare Sub PrintText16J(x As Integer, y As Integer, tex$ As String,P As Integer)
    x++
    y++
	l = Len(tex$)
	Dim Tex1$ As String
	dim Chiffre(2) as integer
	Tex1$=tex$
    DataPtr& = VarPtr&(tex$)
	Chiffre(1)=val(mid$(Tex1$,0,1))+1

	SetGfxPlane Scroll_A
    read t
    if t=32 then 
		DRAWTILESINC2 AdresseChiffre+Palette(0)+priority(0),x,y,2,2
    else
		DRAWTILESINC2 AdresseChiffre+Palette(0)+(Chiffre(1)<<2)+priority(0),x,y,2,2
	end if

End Sub

Declare Sub PrintText16(x As Integer, y As Integer, tex$ As String,P As Integer)
    x++
    y++
	l = Len(tex$)
	Dim Tex1$ As String
	dim Chiffre(2) as integer
	Tex1$=tex$
    DataPtr& = VarPtr&(tex$)
    'VL=val(tex$)
	if l=1 then
		Chiffre(1)=0
		Chiffre(2)=val(mid$(Tex1$,0,1))+1
	else
		Chiffre(1)=val(mid$(Tex1$,0,1))+1
		Chiffre(2)=val(mid$(Tex1$,1,1))+1
	end if

	SetGfxPlane Scroll_A
	'if VL>0 then b=2 else b=1
	For i=1 To 2
	    read t
	    if t=32 then 
			DRAWTILESINC2 AdresseChiffre+Palette(0)+priority(0),x,y,2,2
	    else
			DRAWTILESINC2 AdresseChiffre+Palette(0)+(Chiffre(i)<<2)+priority(0),x,y,2,2
		end if
		x+=2
	Next
End Sub



