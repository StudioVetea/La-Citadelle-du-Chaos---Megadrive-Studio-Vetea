'//////////////////////////
DefiFantastique:
    if versionlangue=0 then AdrB = 191 else AdrB=202
    palettes paletteblack,1,0,16
    palettes paletteblack,2,0,16
    'palettes PaletteDefiA,2,0,16
    'palettes PaletteDefiENGB,1,0,16
    if versionlangue=0 then loadtiles DefiFRTILE_B,AdrB,160 else loadtiles DefiENGTILE_B,AdrB,160
    loadtiles DefiTILE_A,52,160+AdrB
    disable screen
    setgfxplane Scroll_B
    if versionlangue=0 then Reload DefiFRMAP_B else  Reload DefiENGMAP_B         
    For Y = 1 To 28                
      For X = 1 To 40
      ReadInt AuxInt
      DrawTile 160+AuxInt+palette(1), X-1, Y-1
      Next
    Next
    setgfxplane Scroll_A
    Reload DefiMAP_A            
    For Y = 1 To 28                
      For X = 1 To 40
      ReadInt AuxInt
      DrawTile 160+AdrB+AuxInt+palette(2), X-1, Y-1
      Next
    Next
    
    enable screen

    fadeinEx lblptr&(PaletteDefiA),1,3,2

    sleep 100

    fadeOutEx lblptr&(PaletteDefiA),1,3,2
    return

