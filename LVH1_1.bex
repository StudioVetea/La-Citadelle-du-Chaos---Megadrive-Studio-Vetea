Gestion_Inventaire_Use:
    Dim TamponInventaire(20) as integer
    
    UseInventaire=0
    'Init
    for i=0 to 20
        TamponInventaire(i)=0
    next
    
    'Traitement Inventaire
    for i=0 to 20
        if TypeObjetInventaire(i)>0 then
            TamponInventaire(UseInventaire)=TypeObjetInventaire(i)
            UseInventaire++
        end if
    next
    
    'Copie Inventaire
    for i=0 to 20
        TypeObjetInventaire(i)=TamponInventaire(i)
        GestionInventaire TypeObjetInventaire(i),i
    next
    return


 
