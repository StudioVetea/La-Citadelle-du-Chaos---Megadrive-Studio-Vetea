			end if
		end if
		
		change=0
		GoSort=0
		Sleep 1,TvBlank
		
	Loop

	'Switch apr?avoir quitt?
	if PartieRapide=1 then 
		PartieRapide=0
		return
	end if
	
	ExitPage=2
	freeallsprites
	readpage(Page)=1
	gosub TypeFeuille
	Return


	
	
	
